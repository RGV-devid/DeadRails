  Min = 0,
  Max = 3,
  Increase = 1,
  Default = 0,
  Callback = function(v)
_G.Brightness = v
end})
Tabs.Graphics:AddToggle({
Name = "Fullbright",
Callback = function(v)
_G.Fullbright = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.Fullbright then
game.Lighting.Brightness = _G.Brightness or 0
game.Lighting.GlobalShadows = false
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
end
end)
end})
Tabs.Graphics:AddSection({"OP"})
Tabs.Graphics:AddToggle({
Name = "Hitbox",
Callback = function(v)
if v then
for _,v in ipairs(workspace:GetDescendants()) do
if v.Name == "TurretOutlaw" or v.Name == "RifleOutlaw" or v.Name == "ShotgunOutlaw" or v.Name == "RevolverOutlaw" or v.Name == "Vampire" or v.Name == "Werewolf" or v.Name == "Wolf" or v.Name == "Runner" or v.Name == "Walker" or v.Name == "Banner" then
v.HumanoidRootPart.Size = Vector3.new(5,5,5)
end
end
HitboxEnemies = workspace.DescendantAdded:Connect(function(v)
if v.Name == "TurretOutlaw" or v.Name == "RifleOutlaw" or v.Name == "ShotgunOutlaw" or v.Name == "RevolverOutlaw" or v.Name == "Vampire" or v.Name == "Werewolf" or v.Name == "Wolf" or v.Name == "Runner" or v.Name == "Walker" or v.Name == "Banner" then
v.HumanoidRootPart.Size = Vector3.new(5,5,5)
end
end)
else
HitboxEnemies:Disconnect()
for _,v in ipairs(workspace:GetDescendants()) do
if v.Name == "Outlaw" or v.Name == "Vampire" or v.Name == "Werewolf" or v.Name == "Wolf" or v.Name == "Runner" or v.Name == "Walker" or v.Name == "Banner" then
v.HumanoidRootPart.Size = Vector3.new(1.5,1.5,1.5)
end
end
end
end})
Tabs.Graphics:AddToggle({
Name = "NPCs Lock GUI",
Callback = function(v)
if v then
loadstring(game:HttpGet("https://github.com/RGV-devid/DeadRails/blob/main/NPC_Lock?raw=true"))()
else
for _,v in pairs(game.CoreGui:GetDescendants()) do
if v.Name == "NPC_Lock" then
v:Destroy()
end
end
end
end})
Tabs.Graphics:AddButton({
Name = "TP The Bridge",
Callback = function(v)
local plr = game.Players.LocalPlayer.Character
plr.HumanoidRootPart.CFrame = CFrame.new(-374.5484924316406, -70.22786712646484, -49202.4921875)
end})
Tabs.Players:AddSection({"Players"})
Tabs.Players:AddSlider({
  Name = "Field Of View",
  Min = 70,
  Max = 120,
  Increase = 0.5,
  Default = 70,
  Callback = function(v)
_G.SelectFOV = v
end})
Tabs.Players:AddToggle({
Name = "Field Of View Boost",
Callback = function(v)
_G.FOV = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FOV then
workspace.Camera.FieldOfView = _G.SelectFOV or 70
end
end)
end})
Tabs.Players:AddToggle({
Name = "Noclip",
Callback = function(v)
if v then
        noclipConnection = game:GetService("RunService").Stepped:Connect(function()
            local player = game.Players.LocalPlayer
            if player and player.Character then
                for _, part in pairs(player.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
    end
end})

Tabs.Players:AddSection({"Third Person"})
Tabs.Players:AddSlider({
  Name = "Min Distance",
  Min = 0,
  Max = 10,
  Increase = 1,
  Default = 0,
  Callback = function(v)
_G.MinDistancePerson = v
end})
Tabs.Players:AddSlider({
  Name = "Max Distance",
  Min = 30,
  Max = 150,
  Increase = 1,
  Default = 30,
  Callback = function(v)
_G.MaxDistancePerson = v
end})
Tabs.Players:AddToggle({
Name = "Third Person",
Callback = function(v)
_G.ThirdPerson = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.ThirdPerson then
game.Players.LocalPlayer.CameraMode = "Classic"
game.Players.LocalPlayer.CameraMaxZoomDistance = _G.MaxDistancePerson or 30
game.Players.LocalPlayer.CameraMinZoomDistance = _G.MinDistancePerson or 0
else
game.Players.LocalPlayer.CameraMode = "LockFirstPerson"
end
end)
end})
Tabs.Visual:AddToggle({
Name = "Outlaw ESP",
Callback = function(v)
if v then
for _,v in ipairs(workspace:GetDescendants()) do
if v.Name == "RevolverOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "ShotgunOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "RifleOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "TurretOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
end
end
OutlawESP = workspace.DescendantAdded:Connect(function(v)
wait(1)
if v.Name == "RevolverOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "ShotgunOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "RifleOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
elseif v.Name == "TurretOutlaw" and not v:FindFirstChild("OutlawESP") then
LibraryESP:AddESP(v, "Outlaw", Color3.fromRGB(150, 105, 75), 14, "OutlawESP")
end
end)
else
OutlawESP:Disconnect()
LibraryESP:Delete("OutlawESP")
end
end})
